    "@types/pg": "^8.10.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "next": "^14.2.30",
    "pg": "^8.11.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.0",
    "react-hook-form": "^7.47.0",
    "react-markdown": "^9.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "zod": "^3.22.0"
